{
  "name": "adonis-ace",
  "version": "3.0.8",
  "description": "Ace is a command line tool for adonisjs framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "node --harmony-async-await ./node_modules/.bin/japa",
    "test": "./node_modules/.bin/nyc npm run test:local",
    "test:win": "node --harmony-async-await ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "adonis-fold": "^3.0.3",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "japa": "^1.0.3",
    "japa-cli": "^1.0.1",
    "nyc": "^10.3.2",
    "sinon": "^2.2.0",
    "standard": "^10.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "debug": "^2.6.6",
    "es6-class-mixin": "^1.0.5",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "node-exceptions": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/ace.git"
  },
  "keywords": [
    "cli",
    "adonisjs",
    "adonis",
    "terminal",
    "adonis-ace"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/ace/issues"
  },
  "homepage": "https://github.com/adonisjs/ace#readme",
  "bin": {
    "ada": "examples/index.js"
  }
}
